{
  "info": {
      "title": "News explorer API",
      "version": "1.0.0"
  },
  "servers": [
      {
          "url": "http://localhost:3000"
      }
  ],
  "consumes": [
    "application/json"
],
"produces": [
    "application/json"
],
"securityDefinitions": {
    "bearerAuth": {
        "type": "apiKey",
        "name": "Authorization",
        "in": "header",
        "scheme": "bearer",
        "value": "Bearer <JWT>"
    }
},
"security": [
  {
    "bearerAuth": []
  }
]
}